<h1>{{pageName}}'s Personal Records</h1>
<table id="t01" border="1">
    <thead>
        <tr>
            <th>Category</th>
            <th>Record</th>
        </tr>
    </thead>
    <tbody>
        {{#if rows.length }}
            {{#each rows }}
                <tr>
                    <td>{{this.category}}</td>
                    <td>{{this.value}}</td>
                </tr>
            {{/each}}
        {{else}}
            <tr><td colspan="3"><p class='lead'><em>No records were found.</em></p></td></tr>
        {{/if}}
    </tbody>
</table>
